<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN"> <!--The status logger is used internally by log4j2 components-->
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
        </Console>
    </Appenders>
    <Loggers>
        <Root level="ALL">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
